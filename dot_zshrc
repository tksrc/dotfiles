# ZSH Path
export ZSH="$HOME/.oh-my-zsh"

# Theme
ZSH_THEME=cloud

# ZSH Plugins
plugins=(git zsh-syntax-highlighting zsh-autosuggestions)

# Oh My Zsh
source $ZSH/oh-my-zsh.sh

# Paths
source $HOME/.path

# Aliases
source $HOME/.aliases

# Functions
source $HOME/.functions

# Completion
source $HOME/.completion